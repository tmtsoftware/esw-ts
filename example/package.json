    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "eslint-config-standard": "^16.0.1",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-react": "^7.21.5",
    "pm2": "^4.5.0",
    "prettier": "^2.1.2",
    "snowpack": "^2.16.1",
    "typescript": "^4.0.5"
  }
}
